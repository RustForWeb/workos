{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "schedule:weekly",
    ":prConcurrentLimitNone",
    ":prHourlyLimitNone"
  ],
  "packageRules": [
    {
      "groupName": "serde",
      "matchDatasources": ["crate"],
      "matchPackageNames": ["serde", "serde-*", "serde_*"]
    }
  ]
}
